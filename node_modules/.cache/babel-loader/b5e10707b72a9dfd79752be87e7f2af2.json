{"ast":null,"code":"/* eslint-disable */\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      form: {\n        id: '',\n        name: ''\n      },\n      users: '',\n      updateSubmit: false\n    };\n  },\n\n  mounted() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      axios.get('http://localhost:3000/users').then(res => {\n        this.users = res.data;\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n\n    add() {\n      axios.post('http://localhost:3000/users/', this.form).then(res => {\n        this.load();\n        this.form.name = '';\n      });\n    },\n\n    edit(user) {\n      this.updateSubmit = true;\n      this.form.id = user.id;\n      this.form.name = user.name;\n    },\n\n    update(form) {\n      return axios.put('http://localhost:3000/users/' + form.id, {\n        name: this.form.name\n      }).then(res => {\n        this.load();\n        this.form.id = '';\n        this.form.name = '';\n        this.updateSubmit = false;\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n\n    del(user) {\n      axios.delete('http://localhost:3000/users/' + user.id).then(res => {\n        this.load();\n        let index = this.users.indexOf(form.name);\n        this.users.splice(index, 1);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkBA;AACA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,GAAE;IACJ,OAAM;MACFC,IAAI,EAAE;QACJC,EAAE,EAAE,EADA;QAEJC,IAAI,EAAE;MAFF,CADJ;MAKFC,KAAK,EAAE,EALL;MAMFC,YAAY,EAAE;IANZ,CAAN;EAQD,CAVY;;EAWbC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAbY;;EAcbC,OAAO,EAAE;IACPD,IAAI,GAAE;MACFR,KAAK,CAACU,GAAN,CAAU,6BAAV,EAAyCC,IAAzC,CAA8CC,GAAE,IAAK;QACrD,KAAKP,KAAL,GAAaO,GAAG,CAACX,IAAjB;MACD,CAFC,EAECY,KAFD,CAESC,GAAD,IAAS;QACjBC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MAED,CALC;IAMH,CARM;;IASLG,GAAG,GAAE;MACLjB,KAAK,CAACkB,IAAN,CAAW,8BAAX,EAA2C,KAAKhB,IAAhD,EAAsDS,IAAtD,CAA2DC,GAAE,IAAK;QAC9D,KAAKJ,IAAL;QACA,KAAKN,IAAL,CAAUE,IAAV,GAAiB,EAAjB;MACH,CAHD;IAID,CAdM;;IAePe,I